// Test conditional statements and loops
void main() 
begin
    integer x = 10;
    integer y = 20;
    integer max;
    
    // If-else statement
    if (x > y) 
    begin
        max = x;
    end 
    else 
    begin
        max = y;
    end
    
    // For loop
    integer i;
    integer sum = 0;
    for (i = 1; i <= 10; i = i + 1) 
    begin
        sum = sum + i;
    end
    
    
    
end